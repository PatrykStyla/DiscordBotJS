{
	"name": "discordbot",
	"version": "1.0.0",
	"description": "discord bot",
	"main": "dist/src/index.js",
	"scripts": {
		"start": "ts-node-dev --respawn src/index.ts",
		"test": "echo \"Error: no test specified\" && exit 1",
		"prod": "tsc && node .",
		"css": "npx tailwindcss-cli@latest build ./public/css/main.css -o ./dist/main.css",
		"proto": "pbjs -t static-module -w commonjs -o compiled.js ./src/ProtoBuffer/test.proto --keep-case && npx pbts -o compiled.d.ts compiled.js",
		"bundle": "pbjs -t json-module -w commonjs -o bundle.js ./src/ProtoBuffer/test.proto --keep-case && pbjs -t static-module ./src/ProtoBuffer/test.proto --keep-case | pbts -o bundle.d.ts -"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/PatrykStyla/DiscordBotJS.git"
	},
	"author": "",
	"license": "ISC",
	"bugs": {
		"url": "https://github.com/PatrykStyla/DiscordBotJS/issues"
	},
	"homepage": "https://github.com/PatrykStyla/DiscordBotJS#readme",
	"dependencies": {
		"@discordjs/opus": "^0.3.2",
		"@types/mysql": "^2.15.15",
		"@types/node": "^14.11.2",
		"@types/ws": "^7.4.0",
		"chalk": "^4.1.0",
		"discord-akairo": "^8.1.0",
		"discord.js": "^12.4.1",
		"ffmpeg-static": "^4.2.7",
		"mysql": "^2.18.1",
		"protobufjs": "~6.10.2"
	},
	"devDependencies": {
		"ts-node": "^8.10.2",
		"ts-node-dev": "^1.0.0-pre.63",
		"typescript": "^4.0.3"
	}
}
